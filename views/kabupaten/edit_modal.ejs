<div class="container-fluid">
  <a href="/kabupaten">
    <button type="button" class="btn btn-outline-danger mt-5">Kembali</button>
  </a>
  <div class="row mt-4 text-center ">
      <div class="md-12 text-center">
          <h1>Edit Data Ppovinsi</h1>
      </div>
  </div>

  



<form action="/kabupaten/edit" method="POST">
                    <div class="form-group">
                        <input type="hidden" name="id" class="form-control" id="id" required value="<%= kabupaten.id  %>">
                    </div> 
                    <div class="form-group">
                        <label for="id_kabupaten">ID Kabupaten</label>
                        <input type="text" name="id_kabupaten" class="form-control" id="id_kabupaten" required value="<%= kabupaten.id_kabupaten  %>">
                    </div>
                    <div class="form-group">
                        <label for="nama_kabupaten">Nama Kabupaten</label>
                        <input type="text" name="nama_kabupaten" class="form-control" id="nama_kabupaten" required value="<%= kabupaten.nama_kabupaten  %>">
                    </div>
                    <div class="form-group">
                        <label for="id_provinsi">ID Provinsi</label>
                        <input type="text" name="id_provinsi" class="form-control" id="id_provinsi" required value="<%= kabupaten.id_provinsi  %>">
                    </div>

<div mt-5>
  <button type="submit" class="btn btn-primary mt-4">Update</button>
</div>
</form>
</div>
