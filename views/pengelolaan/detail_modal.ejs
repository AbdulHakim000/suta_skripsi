    <h5 class="card-title"><b>Nama Tahanan :</b> <%= pengelolaan.nama_tahanan  %></h5>
    <h5 class="card-title"><b>Perkara :</b><%= pengelolaan.perkara  %></h5>
    <h5 class="card-title"><b>Tanggal Lahir :</b><%= pengelolaan.tgl_lahir  %></h5>
